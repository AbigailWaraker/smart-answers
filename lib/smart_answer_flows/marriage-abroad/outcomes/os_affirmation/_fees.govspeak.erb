<%= render partial: 'consular_fees_table.govspeak.erb',
           locals: { calculator: calculator } %>


<% unless calculator.country_without_consular_facilities? ||
   calculator.ceremony_country == 'indonesia' ||
   calculator.ceremony_country == 'monaco' ||
   calculator.ceremony_country == 'france' ||
   calculator.ceremony_country == 'new-caledonia' ||
   calculator.ceremony_country == 'wallis-and-futuna' %>

  <% if calculator.ceremony_country == 'vietnam' %>
    <%= render partial: 'link_to_consular_fees_pay_in_sterling_or_local_currency.govspeak.erb',
               locals: { calculator: calculator } %>
  <% elsif calculator.ceremony_country != 'cambodia' %>
    <%= render partial: 'link_to_consular_fees.govspeak.erb',
               locals: { calculator: calculator } %>
  <% end %>

  <%= render partial: 'payment_information.govspeak.erb',
             locals: { calculator: calculator } %>
<% end %>
