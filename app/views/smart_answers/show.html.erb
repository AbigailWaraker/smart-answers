<% content_for :title do %><%= @presenter.title %><% end %>
<main id="main" role="main">
  <div id="content" <% if @presenter.started? %> class="smart-answer-questions group"<% end %>>
    <%= render partial: "content" %>
  </div>

  <div class="meta-wrapper<% if @presenter.started? && !@presenter.finished? %> question-meta<% end %>">
    <div id="report-a-problem"></div>
    <%= render :partial => 'smartanswer_metadata' %>
  </div>
</main>
