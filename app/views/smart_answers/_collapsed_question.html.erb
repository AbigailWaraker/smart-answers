<li class="done">
  <% if question_page.title %>
    <h3 class="question">
      <%= question_page.title %>
    </h3>
  <% end %>
  <% question_page.questions.each_with_index do |question, question_index|%>
    <h3 class="question">
      <%= question.title %>
    </h3>
    <%- if question.multiple_responses? -%>
      <div class="answer multiple">
        <ul>
          <%- question.response_labels(@presenter.accepted_responses[number_questions_so_far + question_index]).each do |label| -%>
            <li><%= label %></li>
          <%- end -%>
        </ul>
      </div>
    <%- else -%>
      <div class="answer">
      <%= question.response_label(@presenter.accepted_responses[number_questions_so_far + question_index]) %></div>
    <%- end -%>
  <% end %>
  <p class="undo">
    <% if question_page.questions.count == 1 %>
      <%= link_to "Change", @presenter.change_collapsed_question_link(number_questions_so_far + 1) %>
    <% else %>
      <%= link_to "Change", @presenter.change_collapsed_question_link(number_questions_so_far + 1, question_page.questions.count) %>
    <% end %>
  </p>
</li>
